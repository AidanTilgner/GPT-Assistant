{
  "settings": {},
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": [0, { "caseSensitiveStrict": true }],
    "import/extensions": [0, "never"],
    "quotes": ["error", "double"],
    "no-use-before-define": [0, "never"],
    "no-underscore-dangle": [0, "never"],
    "prefer-regex-literals": [0, "never"],
    "import/prefer-default-export": [0, "never"],
    "react/destructuring-assignment": [0, "always"],
    "prefer-arrow-callback": [0, "never"],
    "func-names": [0, "never"],
    "import/no-extraneous-dependencies": [0, "never"],
    "camelcase": [0, "never"],
    "radix": [0, "never"],
    "no-console": ["warn", { "allow": ["warn", "error", "info"] }],
    "no-unused-vars": "warn"
  }
}
